{
  "name": "assetmanagement",
  "version": "1.0.0",
  "description": "Backend for Asset Management",
  "main": "index.js",
  "scripts": {
    "start": "cross-env NODE_ENV=production PORT=5000 && npm run server:prod",
    "server:prod": "node dist/app.js",
    "build": "babel app.js --out-dir build",
    "clean": "rimraf dist",
    "server": "cross-env DEBUG=app nodemon --exec babel-node app.js",
    "dev": "cross-env NODE_ENV=development PORT=5000 npm-run-all server",
    "prod": "npm-run-all clean build",
    "format": "prettier --write \"./**/*.js\"",
    "client": "cd ../ && npm start --prefix groupactivity",
    "all": "concurrently -n 'server,client' -c 'blue,yellow' \"yarn run dev\" \"npm run client\""
  },
  "author": "Group 3",
  "license": "MIT",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "colors": "^1.4.0",
    "concurrently": "^5.3.0",
    "cors": "^2.8.5",
    "cross-env": "^7.0.2",
    "crypto-random-string": "^3.3.0",
    "ejs": "^3.1.5",
    "express": "^4.17.1",
    "express-fileupload": "^1.2.0",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.10.11",
    "morgan": "^1.10.0",
    "nodemailer": "^6.4.17",
    "passport": "^0.4.1",
    "passport-facebook": "^3.0.0",
    "passport-google-oauth20": "^2.0.0",
    "passport-google-plus-token": "^2.1.0",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.10.5",
    "@babel/core": "^7.12.3",
    "@babel/node": "^7.10.5",
    "@babel/preset-env": "^7.12.1",
    "babel-loader": "^8.1.0",
    "nodemon": "^2.0.2",
    "npm-run-all": "^4.1.5"
  }
}
